# DeltaStream Authentication
# Get your API token from: https://app.deltastream.io/integrations
DELTASTREAM_TOKEN=your_api_token_here

# Get your Organization ID from: https://app.deltastream.io/settings
DELTASTREAM_ORG_ID=your_org_id_here

# DeltaStream API endpoint (default)
DELTASTREAM_SERVER_URL=https://api.deltastream.io/v2

# Database and Schema Configuration
DELTASTREAM_DATABASE_NAME=online_retail_dev
DELTASTREAM_SCHEMA_NAME=public

# Entity Configuration
# This is the entity (Kafka topic) where sales data will be inserted
ENTITY_NAME=fact_sales_raw

# Store Configuration
# Use 'trial_store' for testing, or your own Kafka store name
STORE_NAME=kafka_store
